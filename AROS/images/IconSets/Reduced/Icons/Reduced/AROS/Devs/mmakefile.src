#MM- iconset-Reduced-wbench-icons-aros : iconset-Reduced-wbench-icons-aros-devs iconset-Reduced-wbench-icons-aros-storage

%build_icons mmake=iconset-Reduced-wbench-icons-aros-devs icons=$(DEVSICONS) \
             dir=$(AROS_DEVS)

%build_icons mmake=iconset-Reduced-wbench-icons-aros-storage icons=$(STORAGEICONS) \
             dir=$(AROSDIR)/Storage
