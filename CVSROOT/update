#!/bin/sh

gawk -f filter.awk passwd.txt
cvs com passwd passwd.txt developers

cp passwd passwd.txt developers ~aros/CVS/CVSROOT

gawk -f cvspwtohtpw.gawk passwd.txt $HOME/.htpasswd ../AROS/docs/html/jobserver/.htaccess

MSQL=/usr/local/Hughes/bin/msql

if [ -x $MSQL ]; then
    gawk -f pw2msql.gawk passwd.txt | \
    $MSQL -h aros.fh-konstanz.de jobserv
fi
